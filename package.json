		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.7",
		"@types/node": "^22.13.14",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.3.3",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
