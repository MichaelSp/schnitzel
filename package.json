{
	"name": "schnitzel",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"start": "vite dev",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
